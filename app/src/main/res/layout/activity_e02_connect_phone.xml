<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_connect_phone"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.helge.arrhythmiapt.e02ConnectPhone">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:srcCompat="@drawable/e02connectphone"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true"
        android:id="@+id/imageView3" />

    <Button
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:id="@+id/gotoe03"
        android:layout_height="60dp"
        android:background="@null"
        android:layout_width="100dp"
        android:onClick="gotoe03" />

    <Button
        android:layout_width="100dp"
        android:layout_height="60dp"
        android:layout_marginTop="9dp"
        android:id="@+id/gotoe01"
        android:layout_alignTop="@+id/gotoe03"
        android:layout_alignParentStart="true"
        android:background="@null"
        android:onClick="gotoe01" />

</RelativeLayout>
