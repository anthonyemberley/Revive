<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_f00_questions"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.pink2016.revive.f00Questions">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:srcCompat="@drawable/f00questions"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true"
        android:id="@+id/imageView18"
        android:scaleType="fitXY" />

    <Button
        android:layout_width="80dp"
        android:layout_height="60dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:id="@+id/gotob02"
        android:background="@null"
        android:onClick="gotob02" />

    <Button
        android:layout_width="match_parent"
        android:id="@+id/gotof05"
        android:background="@null"
        android:onClick="gotof05"
        android:layout_height="105dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginStart="6dp"
        android:layout_marginBottom="41dp" />

    <Button
        android:layout_width="match_parent"
        android:id="@+id/gotof04"
        android:background="@null"
        android:onClick="gotof04"
        android:layout_height="100dp"
        android:layout_above="@+id/gotof05"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="8dp" />

    <Button
        android:layout_width="match_parent"
        android:id="@+id/gotof01"
        android:background="@null"
        android:onClick="gotof01"
        android:layout_height="105dp"
        android:layout_below="@+id/gotob02"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="44dp" />

    <Button
        android:layout_width="match_parent"
        android:id="@+id/gotof03"
        android:background="@null"
        android:onClick="gotof03"
        android:layout_height="105dp"
        android:layout_marginTop="18dp"
        android:layout_below="@+id/gotof02"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="match_parent"
        android:id="@+id/gotof02"
        android:background="@null"
        android:onClick="gotof02"
        android:layout_height="100dp"
        android:layout_marginTop="15dp"
        android:layout_below="@+id/gotof01"
        android:layout_alignParentStart="true" />
</RelativeLayout>
